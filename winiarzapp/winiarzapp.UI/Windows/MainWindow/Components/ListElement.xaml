<UserControl x:Class="winiarzapp.UI.Windows.MainWindow.Components.ListElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:winiarzapp.UI.Windows.MainWindow.Components"
             mc:Ignorable="d"
             Height="60" Width="280">
    <Grid Background="DarkGray" Margin="5,5,5,5">
        <Label x:Name="LabelListElement" Content="{}" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="35" Width="255"  Background="White" MouseDoubleClick="LabelListElement_MouseDoubleClick"/>
    </Grid>
</UserControl>
